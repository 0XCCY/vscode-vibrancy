{
	"name": "vscode-custom-css",
	"displayName": "Custom CSS",
	"description": "Custom CSS for Visual Studio Code",
	"version": "1.0.1",
	"publisher": "be5invis",
	"author": {
		"email": "belleve@typeof.net",
		"name": "Belleve Invis",
		"url": "https://typeof.net"
	},
	"engines": {
		"vscode": "^1.3.0"
	},
	"categories": [
		"Other",
		"Themes"
	],
	"preview": true,
	"icon": "images/logo.png",
	"galleryBanner": {
		"color": "#EEEEEE"
	},
	"activationEvents": [
		"*"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.installCustomCSS",
				"title": "Enable Custom CSS"
			},
			{
				"command": "extension.uninstallCustomCSS",
				"title": "Disable Custom CSS"
			}
		]
	},
	"devDependencies": {
		"mustache": "^2.2.1",
		"ncp": "^2.0.0",
		"underscore": "^1.8.3",
		"vscode": "^0.11.0"
	},
	"dependencies": {
		"extract-zip": "^1.5.0",
		"replace-in-file": "^1.0.2",
		"request": "^2.72.0"
	}
}